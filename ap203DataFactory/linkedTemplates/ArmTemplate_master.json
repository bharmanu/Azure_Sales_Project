{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ap203DataFactory"
		},
		"ls_ap203blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ap203blobstorage'",
			"defaultValue": "DefaultEndpointsProtocol=https;AccountName=ap203blobstorage;"
		},
		"ls_ap203keyvault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ap203keyvault1.vault.azure.net/"
		},
		"ls_ap203storageaccount_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ap203storageaccount.dfs.core.windows.net/"
		},
		"ls_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ap203DataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ap203blobstorage_connectionString": {
						"value": "[parameters('ls_ap203blobstorage_connectionString')]"
					},
					"ls_ap203keyvault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_ap203keyvault1_properties_typeProperties_baseUrl')]"
					},
					"ls_github_properties_typeProperties_url": {
						"value": "[parameters('ls_github_properties_typeProperties_url')]"
					},
					"ls_ap203storageaccount_properties_typeProperties_url": {
						"value": "[parameters('ls_ap203storageaccount_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}